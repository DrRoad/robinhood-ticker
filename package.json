{
  "name": "RH-Ticker",
  "version": "2.2.0",
  "license": "MIT",
  "description": "Robinhood Ticker",
  "repository": "https://github.com/peniqliotuv/robinhood-ticker",
  "main": "main.js",
  "scripts": {
    "prod-test": "NODE_ENV=production electron .",
    "start": "NODE_ENV=development electron .",
    "debug": "NODE_ENV=development electron --inspect=5858 .",
    "pack": "NODE_ENV=production electron-builder --dir",
    "dist": "NODE_ENV=production electron-builder",
    "publish": "NODE_ENV=production build -p always"
  },
  "keywords": [
    "Robinhood"
  ],
  "author": "Jerry Tsui",
  "build": {
    "appId": "robinhoodticker",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icon-512.png"
    },
    "win": {
      "icon": "build/logo.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false
    },
    "dmg": {
      "backgroundColor": "#61CA9D"
    }
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "~1.7.8",
    "electron-builder": "^19.55.2",
    "electron-builder-squirrel-windows": "^20.2.0",
    "electron-debug": "^1.5.0",
    "electron-reload": "^1.2.2",
    "nodemon": "^1.14.11"
  },
  "dependencies": {
    "about-window": "^1.9.1",
    "chart.js": "^2.7.1",
    "chartjs-plugin-zoom": "^0.6.3",
    "electron-log": "^2.2.14",
    "electron-store": "^1.3.0",
    "electron-updater": "^2.20.1",
    "menubar": "^5.2.3",
    "moment-timezone": "^0.5.14",
    "node-fetch": "^1.7.3",
    "query-string": "^5.1.0"
  }
}
