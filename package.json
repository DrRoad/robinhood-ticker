{
  "name": "RH-Ticker",
  "version": "1.3.0",
  "license": "MIT",
  "description": "Robinhood Ticker",
  "repository": "https://github.com/peniqliotuv/robinhood-ticker",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "debug": "NODE_ENV=development electron --inspect=5858 .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Jerry Tsui",
  "build": {
    "appId": "robinhoodticker",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icon-512.png"
    }
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "~1.7.8",
    "electron-builder": "^19.55.2",
    "electron-debug": "^1.5.0",
    "electron-reload": "^1.2.2",
    "nodemon": "^1.14.11"
  },
  "dependencies": {
    "about-window": "^1.9.1",
    "chart.js": "^2.7.1",
    "chartjs-plugin-zoom": "^0.6.3",
    "electron-log": "^2.2.14",
    "electron-store": "^1.3.0",
    "electron-updater": "^2.20.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "node-fetch": "^1.7.3",
    "query-string": "^5.1.0"
  }
}
